    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:background="@color/colorTextHint">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/fontSizeNormal"
            android:text="@string/icon_menu"
            android:textColor="@color/textDefault"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:id="@+id/cuoc_btntoLeftMenu"
            android:clickable="true"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/fontSizeTitleMain"
            android:textColor="@color/textDefault"
            android:text="Thông báo cước"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:id="@+id/cuoc_titleMainView"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/fontSizeNormal"
            android:textColor="@color/textDefault"
            android:text="@string/icon_circle"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/cuoc_btntoNext"
            android:clickable="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border_bottom_default"
        android:paddingBottom="30dp"
        android:paddingTop="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:id="@+id/textView2"
            android:layout_alignParentLeft="@+id/cthd_sts"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="45dp"
            />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/cuoc_link1"
        android:gravity="right"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp" />
</RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border_bottom_default"
        android:paddingBottom="10dp"
        android:paddingTop="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Thông báo tiền điện kì 1 tháng 6"
            android:textSize="20dp"
            android:textColor="@color/textDefault"
            android:layout_marginLeft="10dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="(!)"
            android:gravity="right"
            android:textSize="20dp"
            android:id="@+id/cuoc_dien1"
            android:textColor="@color/textDefault"
            android:layout_alignParentRight="true"
            android:layout_marginRight="1dp" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border_bottom_default"
        android:paddingBottom="10dp"
        android:paddingTop="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Thông báo tiền nước tháng 5"
            android:textSize="20dp"
            android:textColor="@color/textDefault"
            android:layout_marginLeft="10dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="(!)"
            android:gravity="right"
            android:textSize="20dp"
            android:id="@+id/cuoc_nuoc1"
            android:textColor="@color/textDefault"
            android:layout_alignParentRight="true"
            android:layout_marginRight="1dp" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border_bottom_default"
        android:paddingBottom="10dp"
        android:paddingTop="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Thông báo tiền nước tháng 6"
            android:textSize="20dp"
            android:textColor="@color/textDefault"
            android:layout_marginLeft="10dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="(!)"
            android:gravity="right"
            android:textSize="20dp"
            android:id="@+id/cuoc_nuoc2"
            android:textColor="@color/textDefault"
            android:layout_alignParentRight="true"
            android:layout_marginRight="1dp" />
    </RelativeLayout>

</LinearLayout>
